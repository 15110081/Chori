
<!-- block -->
<div class="block">
	<div class="navbar navbar-inner block-header">
		<div class="muted pull-left">Manual Barge Operation Grab
			Information</div>
	</div>
	<div class="block-content collapse in">
		<div class="form-horizontal">
			<fieldset>


				<div class="row-fluid padd-bottom padd-top">
					<div class="span3 control-group">
						<label class="control-label">Vessel</label>
						<div class="controls">
							<input class="input-small disabled" id="etb" type="text"
								placeholder="Golden Fortune" disabled>
						</div>
					</div>
					<div class="span3 control-group">
						<label class="control-label">Voyage</label>
						<div class="controls">
							<input class="input-small disabled" id="etb" type="text"
								placeholder="N12345" disabled>
						</div>
					</div>
				</div>

				<div class="row-fluid padd-bottom padd-top">
					<div class="span3 control-group">
						<label class="control-label">Barge Name</label>
						<div class="controls">
							<input class="input-small disabled" id="etb" type="text"
								placeholder="SG12345" disabled>
						</div>
					</div>

				</div>
				<div class="row-fluid padd-bottom padd-top">
					<div class="span3 control-group">
						<label class="control-label">Grab No</label>
						<div class="controls">
							<input class="input-small disabled" id="etb" type="text"
								placeholder="1" disabled>
						</div>
					</div>

				</div>

				<div class="row-fluid padd-bottom padd-top">
					<div class="span3 control-group">
						<label class="control-label">Date Time</label>
						<div class="controls">
							<input class="input-small" id="etb" type="text">
						</div>
					</div>

				</div>
				<div class="row-fluid padd-bottom padd-top">
					<div class="span3 control-group">
						<label class="control-label">Weight</label>
						<div class="controls">
							<input class="input-small" id="etb" type="text">
						</div>
					</div>

				</div>
				<div class="row-fluid padd-bottom">
						<div class="span1 control-group">
							<label class="control-label">Update Status</label>
							
						</div>
						<div class="span3 control-group">

							<div class="controls">
								<label class="control-label"><input type="radio"
									name="barge" class="span2" />Open</label>
							</div>
						</div>
						<div class="span3 control-group">
							<div class="controls">
								<label class="control-label"><input type="radio"
									name="barge" class="span2" />Under Ops</label>
							</div>
						</div>
						<div class="span3 control-group">
							<div class="controls">
								<label class="control-label"><input type="radio"
									name="barge" class="span2" />Finished</label>
							</div>
						</div>
					</div>
				<div class="form-actions">
					<button type="submit" class="btn btn-primary">Save changes</button>
					<button type="reset" class="btn">Cancel</button>
				</div>
			</fieldset>
		</div>
	</div>
</div>

<!-- /block -->

